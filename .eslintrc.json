{
	"extends": ["next/core-web-vitals", "prettier"],
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	},
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 2020,
		"project": "./tsconfig.json",
		"ecmaFeatures": {
			"jsx": true
		},
		"sourceType": "module"
	}
}
